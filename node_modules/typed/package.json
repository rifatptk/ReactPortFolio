{
  "name": "typed",
  "version": "3.1.0",
  "description": "A blazing fast, dependency free, 1kb runtime type-checking library written entirely in typescript.",
  "main": "dist/typed.js",
  "module": "dist/typed.mjs",
  "typings": "dist/typed.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "benchmark": "npm run build && node ./benchmarks",
    "build": "rimraf dist && rollup -c",
    "docs": "typedoc src/index.ts",
    "_postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "test": "jest"
  },
  "keywords": [
    "array",
    "assert",
    "check",
    "checker",
    "collection",
    "data",
    "error",
    "inference",
    "interface",
    "invalid",
    "joi",
    "json",
    "list",
    "model",
    "object",
    "schema",
    "type",
    "typescript",
    "valid",
    "validate",
    "validation",
    "validation",
    "validator"
  ],
  "license": "MIT",
  "author": "Gabriel Vaquer <brielov@icloud.com>",
  "homepage": "https://github.com/brielov/typed#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brielov/typed.git"
  },
  "bugs": {
    "url": "https://github.com/brielov/typed/issues"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "benchmark": "^2.1.4",
    "esbuild": "^0.14.12",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.4.7",
    "jest-esm-transformer": "^1.0.0",
    "lint-staged": "^12.2.2",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.65.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "superstruct": "^0.15.3",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5",
    "zod": "^3.11.6"
  }
}
